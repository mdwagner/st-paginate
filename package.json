{
  "name": "st-paginate",
  "version": "0.0.1",
  "description": "A no-dependency port of the ReactJS library 'ReactPaginate'",
  "main": "dist/stpaginate.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "browser": "dist/stpaginate.js",
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "pretest:ci": "rimraf reports",
    "test:ci": "jest --ci --runInBand --testResultsProcessor='jest-junit'",
    "test:watch": "jest --watch --no-cache"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "^0.7.22",
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1",
    "jest-junit": "^3.6.0",
    "rimraf": "^2.6.2"
  },
  "author": "mdwagner",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdwagner/st-paginate.git"
  },
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "jest-junit": {
    "output": "reports/junit/js-test-results.xml"
  }
}
