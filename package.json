{
  "name": "st-paginate",
  "version": "0.0.3",
  "description": "A no-dependency port of the ReactJS library 'ReactPaginate'",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "jest-junit": {
    "output": "reports/junit/unit-tests.xml"
  },
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watchAll",
    "test:e2e": "stencil test --spec --e2e",
    "test:e2e:watch": "stencil test --spec --e2e --watchAll",
    "test:ci": "stencil test --spec --e2e --ci --reporters=default --reporters=jest-junit"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "~0.16.2",
    "@types/jest": "23.3.14",
    "@types/puppeteer": "1.6.4",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-junit": "6.3.0",
    "puppeteer": "1.8.0"
  },
  "license": "MIT"
}
